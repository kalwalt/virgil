// All the syntactic infix operators.
enum V3Infix(syntax: string, prec: byte, compound: bool) {
	EqEq	("==",  6, false),
	NotEq	("!=",  6, false),
	Add	("+",  13, true),
	Sub	("-",  13, true),
	Mul	("*",  14, true),
	Div	("/",  14, true),
	Mod	("%",  14, true),
	And	("&",   5, true),
	Or	("|",   3, true),
	AndAnd	("&&",  2, true),
	OrOr	("||",  1, true),
	Xor	("^",  4, true),
	Lt	("<",  8, true),
	Gt	(">",  8, true),
	LtEq	("<=",  8, false),
	GtEq	(">=",  8, false),
	Shl	("<<", 12, true),
	Sar	(">>", 12, true),
	Shr	(">>>", 12, true),
	LtOr	("<|", 15, false),
	None	("", 0, false),
}
