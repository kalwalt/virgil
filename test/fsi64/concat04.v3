type M {
	def m1() -> u2 { return 0; }
	def m2() -> u2 { return 0; }

	case A {
		def m2() -> u2 { return 3; }
	}
	case B {
	}
	case C {
	}
}

def compute(f: M -> u2) -> u8 {
	var d: M;
	return  f(M.C) <<| f(M.B) <<| f(M.A) <<| f(d);
}

def main(a: bool) -> int {
	return compute(if(a, M.m1, M.m2));
}
